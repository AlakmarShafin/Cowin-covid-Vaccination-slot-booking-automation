var i = 1;                 
function doit(){for(j=document.getElementsByClassName('age-limit').length-1;j>=0;j--){if(document.getElementsByClassName('age-limit')[j].parentElement.parentElement.innerText != "Booked\nCOVAXIN\nAge 18+" &&  document.getElementsByClassName('age-limit')[j].parentElement.parentElement.innerText != "Booked\nCOVISHIELD\nAge 18+" && document.getElementsByClassName('age-limit')[j].parentElement.parentElement.innerText != "Booked\nCOVISHIELD\nAge 45+"  && document.getElementsByClassName('age-limit')[j].parentElement.parentElement.innerText != "Booked\nCOVAXIN\nAge 45+"){document.getElementsByClassName('age-limit')[j].parentElement.parentElement.scrollIntoView();document.getElementsByClassName('age-limit')[j].parentElement.previousElementSibling.previousElementSibling.click();stopCodeByerr+='error';return;}}}
function myLoop() {        
  setTimeout(function() {    doit();
    console.log('try: '+i); document.getElementsByTagName('ion-button')[0].click()     
       
     //Warning Do not try to understand code
    i++;                   
    if (i < 100) {          
      myLoop();             
    }                       
  }, 2000)
}

myLoop();
