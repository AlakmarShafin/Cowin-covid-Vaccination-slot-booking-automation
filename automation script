var i = 1;                  //  set your counter to 1
function doit(){  if(document.getElementsByClassName('vaccine-box2').length){alert('available');}}
function myLoop() {         //  create a loop function
  setTimeout(function() {    doit();
    console.log('try: '+i); document.getElementsByTagName('ion-button')[0].click()     
       
     //your code here
    i++;                    //  increment the counter
    if (i < 100) {           //  if the counter < 100, call the loop function tries 100 times
      myLoop();             //  ..  again which will trigger another
    }                       //  ..  setTimeout()
  }, 5000)
}

myLoop();
